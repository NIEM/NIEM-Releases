    </xs:annotation>
  </xs:element>
  <xs:element name="AdoptiveParentChildAssociationAugmentationPoint" abstract="true">
    <xs:annotation>
      <xs:documentation>An augmentation point for AdoptiveParentChildAssociationType.</xs:documentation>
    </xs:annotation>
  </xs:element>
