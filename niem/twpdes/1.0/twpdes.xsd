    <xsd:simpleContent>
      <xsd:extension base="twpdes:DateAccuracyIndicatorCodeSimpleType">
        <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
