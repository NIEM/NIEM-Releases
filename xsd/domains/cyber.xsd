  <xs:simpleType name="CyberIndicatorPatternDefensiveMeasureCodeSimpleType">
    <xs:annotation>
      <xs:documentation>A data type designating the availability of an action to counteract the indicator of compromise.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="Available from trusted source">
        <xs:annotation>
          <xs:documentation>An action to counteract the indicator of compromise is available and distributed by a trusted source.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Available from untrusted or unknown source">
        <xs:annotation>
          <xs:documentation>An action to counteract the indicator of compromise is available and distributed by an untrusted or unknown source.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Defensive measure not available">
        <xs:annotation>
          <xs:documentation>A defensive measure to counteract the indicator of compromise is not available.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Defensive measure taken successfully">
        <xs:annotation>
          <xs:documentation>An action was taken successfully to counteract the indicator of compromise.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Defensive measure taken unsuccessfully">
        <xs:annotation>
          <xs:documentation>An action was taken unsuccessfully to counteract the indicator of compromise.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Other">
        <xs:annotation>
          <xs:documentation>Other defensive measure action taken.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Proof of Concept (POC) available from trusted source">
        <xs:annotation>
          <xs:documentation>A Proof of Concept (POC) action to counteract the indicator of compromise is available and distributed by a trusted source.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Proof of Concept (POC) available from untrusted or unknown source">
        <xs:annotation>
          <xs:documentation>A Proof of Concept (POC) action to counteract the indicator of compromise is available and distributed by a untrusted or unknown source.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
