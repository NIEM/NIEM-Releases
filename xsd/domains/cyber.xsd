  <xs:complexType name="CyberObservableInternetServiceProviderObjectType">
    <xs:annotation>
      <xs:documentation>A data type for the Internet Service Provider (ISP) of an observable object.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
